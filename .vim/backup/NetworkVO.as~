package com.cecentral.models 
{
    public class NetworkVO
    {
        private var _httpCDN: String;
        private var _rtmpCDN: String;
		private var _assetServer: String;
		private var _assetBasePath: String;
        private var _basePath: String;

        public static function fromObject(o: Object)
        {
			if ( o == null ) return null;
			
            var vo:NetworkVO = new NetworkVO;
			
            vo.httpCDN = o.httpCDN;
            vo.rtmpCDN = o.rtmpCDN;
			vo.assetServer = o.assetServer;
			vo.assetBasePath = o.assetBasePath;
            vo.basePath = o.basePath

            return vo;
        }
		
		public static function fromObjects(os: Array): Array
		{
			var vos:Array = new Array();
			
			for ( var i:int = 0; i < os.length; i++ )
			{
				vos.push(NetworkVO.fromObject(os[i]));
			}
			
			return vos;
		}

		public function get httpCDN():String
		{
			return _httpCDN;
		}

		public function set httpCDN(value:String):void
		{
			_httpCDN = value;
		}
		
		public function get rtmpCDN():String
		{
			return _rtmpCDN;
		}

		public function set rtmpCDN(value:String):void
		{
			_rtmpCDN = value;
		}

		public function get assetServer():String
		{
			return _assetServer;
		}
		
		public function set assetServer(value:String):void
		{
			_assetServer = value;
		}
		
		public function get assetBasePath():String
		{
			return _assetBasePath;
		}
		
		public function set assetBasePath(value:String):void
		{
			_assetBasePath = value;
		}

		public function get basePath():String
		{
			return _basePath;
		}

		public function set basePath(value:String):void
		{
			_basePath = value;
		}
    }
}
